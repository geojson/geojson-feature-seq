 <front>
        <title abbrev="GeoJSONSeq">GeoJSON Text Sequences</title>

        <author initials="S." surname="Gillies"
                fullname="S. Gillies">
            <organization>Mapbox</organization>
            <address>
                <email>sean.gillies@gmail.com</email>
